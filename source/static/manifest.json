{
    "name": "Chrome extension template",
    "version": "0.1",
    "manifest_version": 2,
    "description": "Chrome extension template",
    "browser_action": {
        "default_icon": "icon.png",
        "default_titie": "Title for extension",
        "default_popup": "popup.html"
    },
    "options_page": "options.html",
    "permissions": [
    ],
    "background": {
        "scripts": [
            "eventPage.js"
        ],
        "persistent": false
    }
}
        

        
